{
    "name": "@meshscout/ingestion",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src --ext .ts",
        "proto:compile": "mkdir -p src/generated && pbjs -t static-module -w commonjs -p proto -o src/generated/meshtastic.js meshtastic/mqtt.proto meshtastic/mesh.proto && pbts -o src/generated/meshtastic.d.ts src/generated/meshtastic.js"
    },
    "dependencies": {
        "@prisma/client": "^5.7.0",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.3.1",
        "geolib": "^3.3.4",
        "h3-js": "^4.1.0",
        "mqtt": "^5.3.0",
        "protobufjs": "^7.2.5"
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "eslint": "^8.54.0",
        "prisma": "^5.7.0",
        "protobufjs-cli": "^1.1.2",
        "tsx": "^4.6.2",
        "typescript": "^5.3.0"
    }
}
